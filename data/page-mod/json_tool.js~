function populate (){
    var thing_list      = null;
    var myJson          = {};
    
    thing_list = document.getElementsByClassName("thing");

    for(var i = 0, l = thing_list.length; i < l; ++i) {
        let word    = thing_list[i].children[1].firstChild.children[1].innerHTML.toLowerCase();
        let id      = thing_list[i].getAttribute("data-thing-id");
        let audio   = null;
        if( thing_list[i].children[3].firstChild.children[2].className.contains('disabled')){
                audio = false;
            }
        else{
            audio = true;
            }
        myJson[word] = {"word":word,"id":id,"hasAudio": audio};
        }
    return myJson;
}
var myWord = populate();
console.log(myWord);
if( Object.getOwnPropertyNames(myWord).length > 0){
    self.port.emit("populated", myWord);
}
else{
    self.port.emit("empty");
}
